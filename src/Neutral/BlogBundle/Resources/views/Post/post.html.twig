{% extends "NeutralBlogBundle::layout.html.twig" %}

{% block title %}{{ post.title }} - {{ parent() }}{% endblock title %}

{% block headline %}{{ post.title }}{% endblock headline %}

{% block content_content %}
    {{ post.fullText|raw }}
    <ul>
    {% for tag in post.tags %}
        <li>{{ tag }}</li>
    {% endfor %}
    </ul>
{% endblock content_content %}